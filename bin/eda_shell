proc set_app_var {args} {
  procname
  set vname [lindex $args 0]
  upvar $vname v
  set value [lindex $args 1]

  set v $value
}
proc print_disp {disp} {
  if {$::do_redirect} {
    #puts $::redirect_to
    set kout [open $::redirect_to w]
      puts $kout $disp
    close $kout
# Done redirect. $::do_redirect is enable in redirect proc
    set $::do_redirect 0
  } else {
      puts $kout $disp
  }
}
