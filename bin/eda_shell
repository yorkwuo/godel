#@>parse_proc_arguments
proc parse_proc_arguments {args} {
  set result_name [lindex $args 2]
  upvar $result_name results

  set mylevel [info level]
  set pname [lindex [info level [expr $mylevel - 1]] 0]

  set list1 $::procs_attr::arr($pname)
  set list2 [lindex $args 1]
  #set zipped [lmap a $list1 b $list2 {list $a $b}]
  foreach a $list1 b $list2 {
    lappend zipped [list $a $b]
  }

  foreach i $zipped {
    set key [lindex $i 0]
    set val [lindex $i 1]
    if {$val != ""} {
      set results($key) $val
    } else {
      if {$key == "value"} {
        set results($key) $val
      }
    }
  }
}

proc define_proc_attributes {args} {
  set rest [lassign $args pname]
  
  set loc_define_args [lsearch $args -define_args]
  set all_define_args [lindex $args [expr $loc_define_args + 1]]
 
  foreach define_arg $all_define_args {
    lappend ::procs_attr::arr($pname) [lindex $define_arg 0]
  }

}

proc dputs {vname} {
  puts "      dputs: $vname"
}

#@>procname
proc procname {} {
  global edactrl

  set mylevel   [info level]
  set proc_name [lindex [info level [expr $mylevel - 1]] 0]
  set full_cmd  [info level 1]
  set fileloc   [info script]
  set level     [info level]
  #puts "    CMD: $proc_name...$fileloc"
  if {[info exist edactrl(procname_disp)] && $edactrl(procname_disp)} {
    puts [format "%d%s%-20s ... %s ... %s" $level "    " $proc_name $fileloc $full_cmd]
  }
  #puts [format "%d%s%-20s ... %s" $level "    " $proc_name $fileloc]
}

